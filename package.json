{
  "name": "apparent-altitude",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "rm -rf dist && astro check --minimumFailingSeverity hint && astro build",
    "preview": "astro preview",
    "preview:ci": "rm -rf dist && IS_LOCAL_PREVIEW=true npm run build && IS_LOCAL_PREVIEW=true npm run preview",
    "astro": "astro",
    "db:start": "tsx scripts/startDb.ts",
    "db:start-and-migrate": "node scripts/startDb.js && npm run db:migrate",
    "db:stop": "tsx scripts/stopDb.ts",
    "db:generate-migration-scripts": "drizzle-kit generate",
    "db:migrate": "tsx ./src/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "lint": "npx eslint src test",
    "lint:fix": "npx eslint src test --fix",
    "test": "vitest watch",
    "test:ci": "npm run lint && vitest run",
    "test:ci-vitest": "vitest run"
  },
  "dependencies": {
    "@astrojs/check": "0.9.3",
    "@astrojs/node": "8.3.3",
    "@astrojs/react": "3.6.2",
    "@astrojs/rss": "4.0.7",
    "@astrojs/tailwind": "5.1.0",
    "@astrojs/vercel": "7.8.0",
    "@auth/drizzle-adapter": "1.4.2",
    "@fontsource-variable/montserrat": "5.0.21",
    "@fontsource-variable/public-sans": "5.0.19",
    "@headlessui/react": "2.1.5",
    "@heroicons/react": "2.1.5",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "astro": "4.15.4",
    "auth-astro": "4.1.2",
    "clsx": "2.1.1",
    "drizzle-orm": "0.33.0",
    "framer-motion": "11.5.4",
    "postgres": "3.4.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-select": "5.8.0",
    "tailwindcss": "3.4.10",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.10.0",
    "@testcontainers/postgresql": "10.13.0",
    "@types/eslint__js": "8.42.3",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/node": "20.14.10",
    "@types/uuid": "10.0.0",
    "dotenv": "16.4.5",
    "drizzle-kit": "0.24.2",
    "eslint": "9.10.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "prettier": "3.3.3",
    "testcontainers": "10.13.0",
    "tsx": "4.19.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.4.0",
    "vitest": "2.0.5"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.21.2"
  }
}
